      <div class="usuario-container">
        <% for(let index=0; index < userList.length; index++) { %>
          
            <div class="usuario-bloques">
              
                <img src="/images/<%= userList[index].image %>"></img>
              
              <h4 >
                  <%= userList[index].nombre %> <%= userList[index].apellido %>
              </h4>
              
              <p>
                <label for="precio">Email </label>
                <%= userList[index].email %>
              </p>
              <p>
                <% if (userList[index].isAdmin) { %>
                  Es administrador
                <% } else { %>
                  No es administrador
                 <% } %>
              </p>
              
            </div>

              <!-- eliminar producto -->
            
                
          
                 

